<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.AfanRisandi.headphoneinfo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Headphone Gaming"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UTS">
        <activity android:name="com.AfanRisandi.headphoneinfo.DetailActivity"
            android:exported="true"/>
        <activity android:name="com.AfanRisandi.headphoneinfo.MainActivity"
            android:exported="true"/>
        <activity android:name="com.AfanRisandi.headphoneinfo.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>